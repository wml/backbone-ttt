<h1>Listing games</h1>

<table>
  <tr>
    <th>Status</th>
    <th>Board</th>
    <th>Moves</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @games.each do |game| %>
  <tr>
    <td><%= game.status %></td>
    <td><%= game.board %></td>
    <td><%= game.moves %></td>
    <td><%= link_to 'Show', game %></td>
  </tr>
<% end %>
</table>

